def type(bower)
	{
	  "name": "$artifactID+lowercase",
	  "version": "$version",
	  "dependencies": {
	  		$dependency...[,$NL]

	  },
	  "resolutions": {
			$resolution...[,$NL]
	  }
	}
end

def type(artifact) trigger(dependency)
	"$name": $url
end

def trigger(dependency)
	"$name": "[$url#]$version"
end


def trigger(resolution)
	"$name": "$version"
end